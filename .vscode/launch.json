    "version": "0.2.0",
    "configurations": [
      {
        "name": "Lacam",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build_debug/main",
        // "args": ["-i", "scripts/scen/scen-even/den312d-even-1.scen", "-m", "scripts/map/den312d.map", "-N" ,"2", "-v", "1", "-k", "8", "--time_limit_sec=100", "--neural_flag=false", "--model=models/den312d_2.pt"],
        "args": ["--seed=1", "--num=100", "--map=scripts/map/random-32-32-10.map", "--scen=scripts/scen/scen-even/random-32-32-10-even-1.scen", "--model=models/den312d_2.pt", "--verbose=1", "--neural_flag=False", "--time_limit_sec=60", "--kval=8", "--output=logs/nntest_random-32-32-10.csv"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
        ],
        "preLaunchTask": "Cmake Debug"
      }
  
    ]
